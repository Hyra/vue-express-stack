    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "yarn server | yarn serve",
    "postinstall": "vue-cli-service build",
    "server": "pm2 start pm2_config.json",
    "start": "pm2-runtime start pm2_config.json --env production"
