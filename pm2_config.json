{
  "apps": [
    {
      "name": "dojokeep",
      "script": "./bin/www",
      "watch": [
        "app.js",
        "routes",
        "resolvers",
        "models",
        "schema",
        "subscription",
        "views"
      ],
      "ignore_watch": ["node_modules", "src"],
      "watch_options": {
        "followSymlinks": false
      },
      "exec_mode": "fork"
    }
  ]
}
